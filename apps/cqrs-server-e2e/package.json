{
  "name": "@pooky3/cqrs-server-e2e",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "implicitDependencies": [
      "@pooky3/cqrs-server"
    ],
    "targets": {
      "e2e": {
        "executor": "@nx/jest:jest",
        "outputs": [
          "{projectRoot}/test-output/jest/coverage"
        ],
        "options": {
          "jestConfig": "apps/cqrs-server-e2e/jest.config.ts",
          "passWithNoTests": true
        },
        "dependsOn": [
          "@pooky3/cqrs-server:build",
          "@pooky3/cqrs-server:serve"
        ]
      }
    }
  }
}
